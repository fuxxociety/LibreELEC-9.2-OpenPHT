[Unit]
Description=Set up binfmt for qemu-arm-static

[Service]
Type=oneshot
ExecStart=/bin/true
ExecStartPre=-/bin/sh -c 'mount binfmt_misc -t binfmt_misc /proc/sys/fs/binfmt_misc'
ExecStartPre=-/bin/sh -c 'echo ":qemu-arm:M::\x7fELF\x01\x01\x01\x00\x00\x00\x00\x00\x00\x00\x00\x00\x02\x00\x28\x00:\xff\xff\xff\xff\xff\xff\xff\x00\xff\xff\xff\xff\xff\xff\xff\xff\xfe\xff\xff\xff:/usr/bin/qemu-arm-static:" > /proc/sys/fs/binfmt_misc/register'

[Install]
WantedBy=sysinit.target
